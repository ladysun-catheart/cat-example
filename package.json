{
  "name": "base-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.5",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "express": "^4.17.1",
    "formik": "^2.0.6",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "redux": "^4.0.1",
    "redux-promise": "^0.6.0",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.3",
    "xml": "^1.0.1",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "mock": "node ./mock-server/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "e2e:open": "npx cypress open",
    "e2e:run": "npx cypress run",
    "eject": "react-scripts eject",
    "data:cat": "node ./server/db/generate_data.js --type cat --num 5 --outputDir ./server/db/json --nameFile cat_list.json",
    "data:contact": "node ./server/db/generate_data.js --type contact --num 5 --outputDir ./server/db/json --nameFile contact_list.json",
    "data:clean": "rm -f -r ./server/db/json",
    "data": "yarn run data:cat && yarn run data:contact",
    "db:import:cat": "mongoimport --jsonArray --host localhost:9010 -u nekoAdmin -p nekoAdmin  --db neko --collection cat < ./server/db/json/cat_list.json",
    "db:import:contact": "mongoimport --jsonArray --host localhost:9010 -u nekoAdmin -p nekoAdmin  --db neko --collection cat < ./server/db/json/cat_list.json",
    "db:import": "yarn run db:import:cat && yarn run db:import:contact",
    "db:init": "mongo localhost:9010 -u nekoAdmin -p nekoAdmin ./server/db/init_db.js",
    "db:drop": "mongo localhost:9010 -u nekoAdmin -p nekoAdmin ./server/db/clean_db.js",
    "db:clean": "yarn run db:drop && yarn run data:clean",
    "db": "yarn run db:clean && yarn run db:init && yarn run data && yarn run db:import"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.0",
    "cypress": "^3.8.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "faker": "^4.1.0",
    "minimist": "^1.2.5",
    "redux-devtools-extension": "^2.13.8"
  }
}
